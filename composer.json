{
  "name": "courseticket/sbert-service",
  "description": "Connector with custom sbert API",
  "license": "MIT",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "eduplex",
    "api"
  ],
  "homepage": "https://github.com/courseticket/sbert-service",
  "support": {
    "issues": "https://github.com/courseticket/sbert-service/issues",
    "source": "https://github.com/courseticket/sbert-service"
  },
  "require": {
    "php": ">=7.3",
    "freefri/cake-rest-api": ">=0.2.0",
    "cakephp/cakephp": "^4.4"
  },
  "require-dev": {
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "SbertService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SbertService\\Test\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": false
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
